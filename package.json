{
  "name": "uast-viewer",
  "version": "0.0.10",
  "description": "reusable component for displaying uast",
  "homepage": "https://github.com/bblfsh/uast-viewer",
  "main": "lib/index.js",
  "module": "es/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:cjs": "babel src --out-dir lib --source-maps inline",
    "build:es": "BABEL_ENV=es babel src --out-dir es --source-maps inline",
    "build:css": "node-sass-chokidar styles/ -o dist/",
    "build:docs": "styleguidist build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:cjs": "babel src --out-dir lib --source-maps inline -w",
    "watch:es": "BABEL_ENV=es babel src --out-dir es --source-maps inline -w",
    "watch:css": "node-sass-chokidar styles/ -o dist/ --watch",
    "lint": "eslint 'src/**/*.js'",
    "format": "eslint --fix 'src/**/*.js'",
    "test": "jest src/",
    "start": "styleguidist server"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "codemirror": "^5.37.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-codemirror2": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-styleguidist": "^7.1.0",
    "react-test-renderer": "^16.4.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^3.0.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "codemirror": "^5.37.0",
    "prop-types": ">=15.5.7",
    "react": ">=16.3.0",
    "react-codemirror2": "^5.0.1",
    "react-dom": ">=16.3.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleNameMapper": {
      "\\.(css|saas)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testURL": "http://localhost/"
  }
}
